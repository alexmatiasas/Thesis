@article{shanahanPressureDistributionShear2019,
  title = {The Pressure Distribution and Shear Forces inside the Proton},
  author = {Shanahan, P. E. and Detmold, W.},
  year = {2019},
  month = feb,
  journal = {Physical Review Letters},
  volume = {122},
  number = {7},
  note      = {\href{https://arxiv.org/pdf/1810.07589}{[arXiv]}},
